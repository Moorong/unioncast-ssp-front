<script>
$(function () {
	/*组件初始化*/
	$("[data-toggle='tooltip']").tooltip();
	$("input[name='switch']").bootstrapSwitch({  
        onText:"开",  
        offText:"关",  
        size:"mini",  
        onSwitchChange:function(event,state){  
            if(state==true){  
                $(this).val("1");  
            }else{  
                $(this).val("2");  
            }  
        }  
    }); 
	//下拉菜单
    $(".dropdown").selectpicker({});
    //参考网址 http://blog.csdn.net/zhengxiangwen/article/details/46480687

	/*日历组件*/
	/*$('.form_date').datetimepicker({
        language:  'zh-CN',
        weekStart: 1,
        todayBtn:  1,
		autoclose: 1,
		todayHighlight: 1,
		startView: 2,
		minView: 2,
		forceParse: 0
    });*/
	/*双日历组件*/
	$('#reportrange2 span').html(moment().subtract('days', 7).format('YYYY.MMMM.D') + ' - ' + moment().format('YYYY.MMMM.D'));
	$('#reportrange2').daterangepicker({});
  	
	$("table input[name='checkboxall']").on('click',function(event){
		$(this).parent().parent().parent().next('tbody').find("input").prop('checked',$(this).prop('checked'));
		event.stopPropagation();
	});
	$('table input[name="checkboxed"]').on('click',function(event){
		$(this).parent().parent().parent().prev('thead').find("input[name='checkboxall']").prop('checked', $(this).parent().parent().parent().find('tr').find('input:checked').length == $(this).parent().parent().parent().find('tr').length ? true : false);
		event.stopPropagation();
	});
	
	$('.xiugaibtn').click(function(){
		var text = $(this).prev('input').val();
		$(this).next('button').toggleClass('hidden');
		if($(this).prev('input').prop('disabled')){
			$(this).prev('input').prop('disabled',false);
			$(this).text('保存');	
		}else{
			$(this).prev('input').prop('disabled',true);
			$(this).text('修改');
		}
	});
	$('.quxiaobtn').click(function(){
		var text = $(this).prevAll('input').prop('placeholder');
		$(this).prevAll('input').val(text).prop('disabled',true);
		$(this).addClass('hidden');
	});
});

</script>
