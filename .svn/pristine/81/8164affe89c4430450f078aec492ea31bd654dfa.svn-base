.xuanzechuangyi{
  width: 250px;
  /*height: 220px;*/
  overflow: hidden;
  margin: 32px auto 0;
  position: relative; 
}
.xuanzechuangyi.xuanzechuangyi-add{
  background: #cfcfcf;
  line-height: 220px;
  text-align: center;
  color: #fff;
}
.xuanzechuangyi.xuanzechuangyi-add a{
  display: block;
  height: 220px;
}
.xuanzechuangyi.xuanzechuangyi-add a .iconfont{
  font-size: 60px;
  color: #fff;
}
.xuanzechuangyi img{
  width: 100%;
  height: 195px;
}
.xuanzechuangyi div{
  height: 25px;
  background: #cfcfcf;
  line-height: 25px;
  padding:0 14px;
}
.xuanzechuangyi .xuanzechuangyiicon{
  position: absolute;
  right: 0;
  top: 0;
  width: 0;
  height: 0;
  border-top: 64px solid #83c62f;
  border-left: 56px solid transparent; 
  display: none;
}
.xuanzechuangyi .xuanzechuangyiicon i{
  color: #fff;
  position: absolute;
  right: 1px;
  top: -57px;
  font-size: 24px;
}
.xuanzechuangyi .xuanzechuangyiicon.xuanzechuangyi-ok{
  display: block;
}

