.bootstrap-switch.bootstrap-switch-mini .bootstrap-switch-handle-on{
  border-radius: 50%;
  background: #a9d96c;
}
.bootstrap-switch-label{
  background: #fff;
}
.bootstrap-switch.bootstrap-switch-animate .bootstrap-switch-container{
  background: #fff;
}
.bootstrap-switch.bootstrap-switch-mini.bootstrap-switch-off,
.bootstrap-switch.bootstrap-switch-mini.bootstrap-switch-off .bootstrap-switch-container,
.bootstrap-switch.bootstrap-switch-mini.bootstrap-switch-off .bootstrap-switch-label{
