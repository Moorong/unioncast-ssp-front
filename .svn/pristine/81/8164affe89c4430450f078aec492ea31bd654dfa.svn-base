/* creative-labels */
.creative-labels{
  border:1px solid #ccc;
  padding: 10px 15px;
  font-size: 12px;
}
.creative-labels h4{
  font-weight: bold;
  font-size: 14px;
  color: #000;
  margin-bottom: :10px;
}
.creative-labels h4 a.show{
  float: right;
}
.creative-labels .label-selected{
  border: 1px solid #e1e1e1;
  padding: 10px;
}
.label-show-box,
.label-tab{
  margin-bottom: 5px;
}
.label-show-box label,
.label-tab label{
  float: left;
  margin-top: 15px;
  margin-right: 10px;
}
.label-show,
.label-add{
  float: left;
  padding: 5px 15px;
  border: 1px solid #bebcbc;
  background: #bebcbc;
  color: #000;
  font-size: 12px;
  margin-right: 25px;
  margin-top: 10px;
  border-radius:3px;
  position: relative;
}
.label-show:before,
.label-add:before{
  content: "";
  position: absolute;
  left: -8px;
  top: 7px;
  border-right: 8px solid #bebcbc;
  border-top: 6px solid transparent;
  border-bottom:  6px solid transparent;
}
.label-show i.iconfont,
.label-add i.iconfont{
  font-size: 12px!important;
  color: #000!important;
  margin: 0 5px;
}
.label-add-ok{
  background: #c5c5c5!important;
  color: #a5a5a5!important;
  cursor:default;
}
.label-add-ok i.iconfont{
  color: #a5a5a5!important;
}
input.label-input{
  padding: 5px 10px;
  border:none;
  display: block;
  width: 100%;
  font-size: 14px;
  margin-top: 20px;
}
input.label-input:focus{
  outline:none;
}

