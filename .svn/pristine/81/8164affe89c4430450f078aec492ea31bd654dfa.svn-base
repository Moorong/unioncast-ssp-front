/*菜单*/
.sidebar{
  width: 212px;
  position: fixed;
  left: 0;
  top: 67px;
  bottom: 0;
  overflow: auto;
  background: #242730;
}
.icon{
  width: 16px;
  height: 16px;
  display: inline-block;
  margin-right: 20px;
  vertical-align: middle;
}

.ui_nav{
  padding-top: 20px;
  color: #bbb;
  width: 100%;
  overflow: hidden; 
  background: #242730;
  font-size: 12px;
}
.ui_nav a{
  display:block;
  color: #6f7e7d;
  cursor: pointer;
}
.ui_nav ul{
  margin: 0;
  padding: 0;
}
/*一级菜单*/
 .ui_nav > ul > li > a{
  padding-left: 50px;
  cursor: pointer;
  height:48px;
  line-height: 48px;
  font-size: 16px;
}
.ui_nav > ul > li >.ui_nav_top i{
  padding-right: 12px;
}
/*二级菜单*/
.ui_nav > ul > li > ul.ui_nav_sub_wrap{
  overflow: hidden;
  display: none;
}
.ui_nav > ul > li > ul.ui_nav_sub_wrap a{
  padding-left: 86px;
  display: block;
  height: 40px;
  line-height: 40px;
  font-size: 14px;
}
/*显示隐藏切换效果*/
.ui_nav > ul > li.on > a,
.ui_nav > ul > li > a:hover{
  color: #3daded;
}
.ui_nav > ul > li.on ul.ui_nav_sub_wrap{
  display: block;
}
.ui_nav > ul > li.on > ul li.on a,
.ui_nav > ul > li > ul li a:hover
{
  color: #fff;
  background: #1c1f28;
}
/*右侧*/
.content{
  position: absolute;
  left: 212px;
  top: 67px;
  right: 0;
  bottom: 0;
}
.page_main{
  position: absolute;
  top: 37px;
  bottom: 0;
  left: 0;
  right: 0;
  background: #f0f1f6;
  padding: 10px;
  overflow: auto;
}
.page_main_white{
    background: #fff;
    box-shadow: 0 0 10px rgba(0,0,0,0.2);
    position: absolute;
    bottom: 10px;
    top: 10px;
    left: 10px;
    right: 10px;
}
