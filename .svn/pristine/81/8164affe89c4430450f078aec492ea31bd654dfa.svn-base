/*头部*/
.header{
  height: 67px;
  border-bottom: 1px solid #ccc;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  z-index: 1000;
}
.logogroup{
  float: left;
  margin-top:18px;
  font-size: 18px;
}
.logogroup a{
  color: #000;

}
.logogroup li{
  float: left;
}
.user_info{
  float: right;
  padding-top: 20px;
  margin-right: 51px;
}
.user_info li{
  float: left;
  color: #000;
}
.user_info li span{
  display: inline-block;
  padding: 0 10px;
  height: 20px;
  line-height: 20px;
  color: inherit;
  font-size: 12px;
}
.user_info li a.goout{
  padding: 6px 14px;
  color: inherit;
  line-height: 1.5em;
  font-size: 12px;
  border: 1px solid #000;
  border-radius: 15px;
}
.user_info li a i{
  margin-left: 5px;
}
.glyphicon,
.iconfont{
  margin-right: 5px;
}
