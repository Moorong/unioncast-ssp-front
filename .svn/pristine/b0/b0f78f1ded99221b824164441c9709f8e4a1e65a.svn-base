<script type="text/javascript" src="js/base/moment.js"></script>
<script type="text/javascript" src="js/base/daterangepicker.js"></script>
<script>
$(function () {
	/*缁勪欢鍒濆鍖�*/
	$("[data-toggle='tooltip']").tooltip();
	$("input[name='switch']").bootstrapSwitch({  
        onText:"寮�",  
        offText:"鍏�",  
        size:"mini",  
        onSwitchChange:function(event,state){  
            if(state==true){  
                $(this).val("1");  
            }else{  
                $(this).val("2");  
            }  
        }  
    }); 
	//涓嬫媺鑿滃崟
    $(".dropdown").selectpicker({});
    //鍙傝�冪綉鍧� http://blog.csdn.net/zhengxiangwen/article/details/46480687

	/*鏃ュ巻缁勪欢*/
	/*$('.form_date').datetimepicker({
        language:  'zh-CN',
        weekStart: 1,
        todayBtn:  1,
		autoclose: 1,
		todayHighlight: 1,
		startView: 2,
